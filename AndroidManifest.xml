<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.workwithme_v19"
          android:versionCode="1"
          android:versionName="1.0">

  <uses-sdk
      android:minSdkVersion="19"
      android:targetSdkVersion="21"/>

  <application
      android:allowBackup="true"
      android:icon="@drawable/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme">
    <activity
        android:name="com.workwithme_v19.activity.WelcomeActivity"
        android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name="com.workwithme_v19.activity.LoginActivity"
        android:label="Login"
        ></activity>
    <activity
        android:name="com.workwithme_v19.activity.SignUpActivity"
        android:label="Signup"></activity>
  </application>

</manifest>
